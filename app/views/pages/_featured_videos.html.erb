<div class="vidoes">
	<h1>Featured Videos</h1>

	<div class="col-md-7 no-padding">
		<div class="vid-container">
		   <iframe
		       id="vid_frame"
		       src="//www.youtube.com/embed/wmvYvlpJBGs?rel=0&showinfo=0&autohide=1"
		       frameborder="0" width="100%" height="315px">
		   </iframe>
		</div>
	</div>
	<div class="col-md-5">
		<h4 id="vid-main-title">Greece 2013</h4>
		<p id="vid-main-description">Friends take an action-packed trip to Greece.</p>
	</div>
	<div class="col-md-12">
		<div class="vid-list-container">
			<div class="vid-list">
				<%= render partial: 'pages/video_list' %>
			</div><!--Vid-List-->
		</div><!--Vid-list-container-->

		<div class="arrows">
	    <div class="arrow-left arrow"><i class="glyphicon glyphicon-chevron-left"></i></div>
	    <div class="arrow-right arrow"><i class="glyphicon glyphicon-chevron-right"></i></div>
		</div>
	</div>
</div>
